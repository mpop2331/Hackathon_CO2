    "sap.bootcamp.PersonCredential": {
      "kind": "entity",
      "elements": {
        "credID": {
          "key": true,
          "type": "cds.UUID"
        },
        "person": {
          "type": "cds.Association",
          "target": "sap.bootcamp.Person",
          "keys": [
            {
              "ref": [
                "email"
              ]
            }
          ]
        },
        "password": {
          "type": "cds.String"
        }
      }
    },
    "sap.bootcamp.Person": {
      "kind": "entity",
      "elements": {
        "email": {
          "key": true,
