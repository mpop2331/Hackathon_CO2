        },
        "firstName": {
          "type": "cds.String"
        },
        "lastName": {
          "type": "cds.String"
        },
        "cred": {
          "type": "cds.Composition",
          "target": "sap.bootcamp.PersonCredential",
          "on": [
            {
              "ref": [
                "cred",
                "person"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        },
        "road": {
          "type": "cds.Composition",
          "cardinality": {
            "max": "*"
          },
          "target": "sap.bootcamp.Road",
          "on": [
            {
              "ref": [
                "road",
                "parent"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        },
        "room": {
          "type": "cds.Composition",
          "cardinality": {
            "max": "*"
          },
          "target": "sap.bootcamp.Person2Room",
          "on": [
            {
              "ref": [
                "room",
                "personID"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        }
      }
    },
